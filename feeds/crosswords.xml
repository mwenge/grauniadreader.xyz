        <media:credit scheme="urn:ebu">Photograph: David Montgomery/Getty Images</media:credit>
      </media:content>
      <media:content width="460" url="https://i.guim.co.uk/img/media/f2b8ec3882c409d6c8fecab50f2f95fea2b895d5/1054_461_2912_1748/master/2912.jpg?width=460&amp;quality=85&amp;auto=format&amp;fit=max&amp;s=361e61e869a16966066ba4bc30704773">
        <media:credit scheme="urn:ebu">Photograph: David Montgomery/Getty Images</media:credit>
      </media:content>
      <dc:creator>Alan Connor</dc:creator>
